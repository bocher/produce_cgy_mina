/* pages/home.wxss */
.home {
  padding: 0 30rpx;
}
.head-wrap .shop-search{
  display:flex;
  align-items:center;
  border-radius:30rpx;
  height:70rpx;
  font-size:24rpx;
  color: #999;
  padding: 0 30rpx;
  background-color: #F9F9F9;
  border:2rpx solid rgb(236, 236, 236);
}
.head-wrap .search-wrap{
  width: 40rpx;
  height: 40rpx;
}
.content-item {
  padding: 20rpx 20rpx;
  border: 1rpx dotted #FFD37E;
  background-color: #FFFBF7;
  border-radius: 20rpx;
}
.content-item .item-img {
  width: 50rpx;
  height: 50rpx;
}
image {
  width: 100%;
  height: 100%;
}